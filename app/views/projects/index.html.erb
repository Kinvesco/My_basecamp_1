
<h1>Projects</h1>
<div class="card-body">
  <div id="projects">
  <% @projects.each do |project| %>
    <% if project.user == current_user%>
    <%= render project %>
    <p>
      <%= link_to "Show this project", project, class: "btn btn-primary" %>
    </p>
    <% end %>
  <% end %>
</div>    


</div>


<%= link_to "Add New Project", new_project_path, class: "btn btn-success" %>

